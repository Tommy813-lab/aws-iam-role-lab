{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ReadOnlyAuditor",
            "Effect": "Allow",
            "Action": [
                "s3:ListBucket",
                "s3:GetObject",
                "ec2:Describe*",
                "iam:Get*",
                "iam:List*",
                "cloudwatch:Describe*",
                "cloudwatch:Get*",
                "dynamodb:Scan",
                "dynamodb:Query",
                "dynamodb:Describe*",
                "lambda:Get*",
                "lambda:List*"
            ],
            "Resource": "*"
        }
    ]
}
 